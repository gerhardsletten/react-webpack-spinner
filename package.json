{
  "name": "react-webpack-spinner",
  "version": "1.0.1",
  "description": "A simple react spinner with inline/embeded css",
  "main": "lib/Spinner.js",
  "scripts": {
    "prepublish": "npm run compile",
    "start": "webpack-dev-server --port 3001 --host 0.0.0.0 --progress --colors --hot --content-base ./www --config ./webpack.config.js",
    "build": "webpack -p --verbose --colors --display-error-details --config ./webpack.build.config.js",
    "compile": "webpack --mode=build --config ./webpack.build-pack.config.js",
    "lint": "eslint -c .eslintrc src demo; exit 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gerhardsletten/react-webpack-spinner.git"
  },
  "author": "Eyvind Gerhard Sletten",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gerhardsletten/react-webpack-spinner/issues"
  },
  "homepage": "https://github.com/gerhardsletten/react-webpack-spinner#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "eslint-plugin-standard": "^1.3.1",
    "open-browser-webpack-plugin": "0.0.2",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-prefixr": "^0.1.0"
  }
}
